{
  "name": "phood",
  "version": "0.2.0",
  "description": "A food mobile application that finds you recipes using the Yummly REST API. Built using ES6 syntax and modules, Marionette and Backbone.",
  "main": "app/server.js",
  "dependencies": {
    "express": "~3.2.5",
    "stylus": "~0.32.1",
    "yummly": "~0.2.4",
    "bower": "^1.3.5"
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-jshint": "~0.10.0",
    "load-grunt-config": "^0.13.1",
    "time-grunt": "^1.0.0",
    "grunt-newer": "^0.7.0",
    "grunt-traceur": "^0.2.7",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-contrib-uglify": "^0.5.1",
    "traceur": "0.0.62"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app/server.js",
    "postinstall": "./node_modules/bower/bin/bower cache clean && ./node_modules/bower/bin/bower install",
    "traceur": "./node_modules/traceur/traceur --modules=commonjs --experimental --source-maps --dir app/assets/js/application es5/"
  },
  "repository": "https://github.com/ricca509/phood",
  "keywords": [
    "yummly"
  ],
  "engines": {
    "node": "0.10.x"
  },
  "author": "Riccardo Coppola",
  "license": "BSD"
}
